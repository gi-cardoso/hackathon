generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // ou "mysql" / "sqlite" conforme seu caso
  url      = env("DATABASE_URL")
}

model ProcedimentoSaude {
  id                 Int      @id @default(autoincrement())
  Código             Int
  terminologia       String   // Terminologia de Procedimentos e Eventos em Saúde (Tab. 22)
  correlacao         String  // Correlação (Sim/Não)
  PROCEDIMENTO       String
  resolucaoNormativa String?  // Resolução Normativa (alteração)
  VIGÊNCIA           String? // VIGÊNCIA
  OD                 String?
  AMB                String?
  HCO                String?
  HSO                String?
  PAC                String?
  DUT                String?
  SUBGRUPO           String
  GRUPO              String
  CAPÍTULO           String
}
